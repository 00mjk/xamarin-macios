<!-- All msbuild target logic needs to be inside a project tag -->
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
	    <CreateAppBundleDependsOn>$(CreateAppBundleDependsOn);CreateNativeLibs</CreateAppBundleDependsOn>
	</PropertyGroup>

	<Target Name="CreateNativeLibs" Inputs="../common/mac/SimpleClass.m" Outputs="bin/SimpleClass.dylib;bin/SimpleClassStatic.a">
		<Exec Command="make bin/SimpleClass.dylib bin/SimpleClassStatic.a"/>
	</Target>
</Project>
